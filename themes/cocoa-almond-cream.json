  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#93A0A5",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant"],
      "settings": {
        "foreground": "#587d8a"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.charcter.escape"
      ],
      "settings": {
        "foreground": "#766899"
      }
    },
    {
      "scope": ["entity.name"],
      "settings": {
        "foreground": "#766899"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag",
        "entity.name.namespace",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#29668f"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#cc5c3d",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#b58245"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#758C93"
      }
    },
    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "#587d8a"
      }
    },
