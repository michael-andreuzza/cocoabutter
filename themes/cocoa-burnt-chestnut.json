  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#ffffff50",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant"],
      "settings": {
        "foreground": "#7d96a8"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.charcter.escape"
      ],
      "settings": {
        "foreground": "#e6927a"
      }
    },
    {
      "scope": ["entity.name"],
      "settings": {
        "foreground": "#e6927a"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag",
        "entity.name.namespace",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#847C98"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#588291",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#dfb583"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#ffffff50"
      }
    },
    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "#7d96a8"
      }
    },
    {
      "scope": ["meta.tag", "meta.brace"],
      "settings": {
        "foreground": "#bbb1a7"
      }
    },
    {
